#import <stdio.h>
#import <string.h>
int convert(char rom) {
    char roman[] = "IVXLCDM";
    int out = 1;
    for(int i = 0; i < 7; i++, out *= (i % 2 == 0) ? 2 : 5) if(roman[i] == rom) break;
    return out;
}
int romanToInt(char s[]) {
    int sum = 0;
    for(int i = 0; i < strlen(s); i++) {
        int val = convert(s[i]);
        sum += (i == strlen(s) - 1 || val >= convert(s[i + 1])) ? val : -val;
    }
    return sum;
}
